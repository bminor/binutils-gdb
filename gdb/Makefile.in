	$(INSTALL_XFORM) gdb $(bindir)/gdb
	$(INSTALL_XFORM1) $(srcdir)/gdb.1 $(man1dir)/gdb.1
	$(INSTALL_DATA) $(srcdir)/gdb.1 $(man1dir)/$$n.1
	$(M_INSTALL)
	@$(MAKE) DO=install "DODIRS=$(SUBDIRS)" $(FLAGS_TO_PASS) subdir_do

