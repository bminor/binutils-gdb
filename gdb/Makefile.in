		sed -e 's/.*gdb_target=//' -e 's/ ;;$$/.mh/' | sort -u >TARGconf.o
	(cd ${srcdir}/config; ls *.mt) >TARGdir.o
	diff -u HOSTconf.o HOSTdir.o

