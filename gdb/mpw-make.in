	Echo '};'					>> "{o}"version.new
	Echo ''						>> "{o}"version.new
	Echo "resource 'vers' (2, purgeable) " `open-brace`  >> "{o}"version.new
	Echo "	{major},"				>> "{o}"version.new
	Echo "	{minor},"				>> "{o}"version.new
	Echo "	{release},"				>> "{o}"version.new
	Echo '	0,'					>> "{o}"version.new
