CC_LD = Link

s = "{srcdir}"

o = :

# File in object dir can come from either the current dir or srcdir.
 
"{o}" \Option-f : "{s}"

# Default rule that puts each file into separate segment.

.c.o  \Option-f .c
   {CC}  {DepDir}{Default}.c {INTERNAL_CFLAGS} -s gdb_{Default} -o {TargDir}{Default}.c.o

