    # Print all members of g_vC.

    setup_xfail_format "DWARF 1"
    gdb_test "print g_vC" ".* = \{\<class vA\> = \{va = 7, vx = 8\}, vc = 9, vx = 10, Virtual table at $hex\}" "print g_vC"
}

